{
  "name": "skippbox",
  "version": "0.0.1",
  "description": "Skippbox mobile app",
  "license": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "--"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint src",
    "pre-commit-lint": "LIST=`git diff --name-only --cached | grep .*\\.js$`;if [ \"$LIST\" ]; then eslint $LIST; fi",
    "test": "jest --runInBand",
    "react-upgrade": "react-native upgrade --package=com.azendoo.azmobile"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/react-native/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".+/(resources)/.+",
      ".+/(autoMockOff.js)"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "alt",
      "immutable",
      "lodash"
    ],
    "globals": {
      "React": true
    },
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png$": "RelativeImageStub"
    }
  },
  "dependencies": {
    "@exponent/react-native-action-sheet": "^0.2.0",
    "@exponent/react-native-navigator": "^0.4.7",
    "alt": "0.18.4",
    "alt-container": "1.0.2",
    "alt-utils": "1.0.0",
    "babel-plugin-module-alias": "1.5.0",
    "base-64": "^0.1.0",
    "immutable": "3.8.1",
    "intl": "1.2.4",
    "intl-messageformat": "1.3.0",
    "intl-relativeformat": "1.3.0",
    "lodash": "4.13.1",
    "qs": "6.2.0",
    "react": "15.2.0",
    "react-native": "git+https://github.com/kemcake/react-native.git#https-support",
    "react-native-safari-view": "^1.0.0",
    "react-native-sortable-listview": "0.0.8",
    "react-native-swipeout": "git+https://github.com/kemcake/react-native-swipeout.git"
  },
  "devDependencies": {
    "babel-eslint": "6.1.0",
    "eslint": "3.0.0",
    "eslint-plugin-react": "5.2.2",
    "ghooks": "1.3.2",
    "jest-cli": "13.0.0",
    "react-addons-perf": "15.2.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run pre-commit-lint"
    }
  }
}
